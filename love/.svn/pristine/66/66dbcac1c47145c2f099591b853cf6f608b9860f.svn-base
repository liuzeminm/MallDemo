package cn.pojo;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;
@ApiModel(value="商品类别",description="商品类别排序方式")
public class Brandnotes {
    @ApiModelProperty(value = "商品类别id",required = true)
    private Integer bnid;
    @ApiModelProperty(value = "商品类别",required = true)
    private String bnmerchandisecategories;
    @ApiModelProperty(value = "排序方式",required = true)
    private Integer bnsortord;

    public Integer getBnid() {
        return bnid;
    }

    public void setBnid(Integer bnid) {
        this.bnid = bnid;
    }

    public String getBnmerchandisecategories() {
        return bnmerchandisecategories;
    }

    public void setBnmerchandisecategories(String bnmerchandisecategories) {
        this.bnmerchandisecategories = bnmerchandisecategories == null ? null : bnmerchandisecategories.trim();
    }

    public Integer getBnsortord() {
        return bnsortord;
    }

    public void setBnsortord(Integer bnsortord) {
        this.bnsortord = bnsortord;
    }
}