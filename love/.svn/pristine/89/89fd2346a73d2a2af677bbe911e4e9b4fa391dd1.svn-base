package cn.pojo;

public class Goodscomments {
    private Integer gcid;

    private String gcgoodscomments;

    private Integer gccommodityid;

    public Integer getGcid() {
        return gcid;
    }

    public void setGcid(Integer gcid) {
        this.gcid = gcid;
    }

    public String getGcgoodscomments() {
        return gcgoodscomments;
    }

    public void setGcgoodscomments(String gcgoodscomments) {
        this.gcgoodscomments = gcgoodscomments == null ? null : gcgoodscomments.trim();
    }

    public Integer getGccommodityid() {
        return gccommodityid;
    }

    public void setGccommodityid(Integer gccommodityid) {
        this.gccommodityid = gccommodityid;
    }
}